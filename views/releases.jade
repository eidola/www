extends layout

block content
  #releases
    h2 RELEASES
    -for (var id in items)
    - var item = items[id]
      div.release
        div.title #{item.title} | #{item.Artist}
        div.description
          div.textwrapper 
            p #{item.description}
          div.imgwrapper
            img(src='/images/#{item._id}/cover.jpg')


include footer